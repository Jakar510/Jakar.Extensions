<?xml version="1.0"
      encoding="utf-8"?>

<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
	<metadata>
		<id>Jakar.Xml</id>
		<version>1.0.0</version>
		<title>Extension Library</title>
		<authors>Tyler Stegmaier</authors>
		<owners>Jakar510/Jakar</owners>
		<projectUrl>https://github.com/Jakar510/Jakar.Extensions</projectUrl>
		<iconUrl>https://github.com/Jakar510/Jakar.Extensions/blob/master/images/icon.png</iconUrl>
		<license type="file">LICENSE.txt</license>
		<requireLicenseAcceptance>true</requireLicenseAcceptance>
		<copyright>Copyright (c) 2021</copyright>
		<icon>icon.png</icon>
		<description>
			A PREDICTABLE (de)serializer for any given object. Primary goals are to be easily human readable, predictable and consistent.
		</description>
		<releaseNotes>
			Initial release.
		</releaseNotes>
		<tags>Jakar.Api Extensions</tags>
		<language>en-US</language>
