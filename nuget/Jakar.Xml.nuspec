	<files>
		<file src="..\LICENSE.txt"
		      target="" />

		<file src="..\images\icon.png" />

		<!-- .net standard 2.1 -->
		<file src="../Jakar.Xml/bin/Release/net6.0/Jakar.Xml.dll"
		      target="lib/net6.0" />
		<file src="../Jakar.Xml/bin/Release/net6.0/Jakar.Xml.xml"
		      target="lib/net6.0" />
		<file src="../Jakar.Xml/bin/Release/net6.0/Jakar.Xml.pdb"
		      target="lib/net6.0" />
