		<PackageReference Include="Blazored.Modal" Version="7.1.0" />
		<PackageReference Include="Blazored.SessionStorage" Version="2.3.0" />
		<PackageReference Include="Blazored.Toast" Version="4.1.0" />
		<PackageReference Include="Jakar.Extensions" Version="6.7.20" />
		<PackageReference Include="Jakar.Extensions.Hosting" Version="6.7.2" />
		<PackageReference Include="MailKit" Version="3.6.0" />
		<PackageReference Include="Npgsql" Version="7.0.2" />
		<PackageReference Include="Npgsql.Json.NET" Version="7.0.2" />
		<PackageReference Include="Radzen.Blazor" Version="4.8.3" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\Jakar.Database\Jakar.Database.csproj" />
