	<ItemGroup>
		<None Include="..\.editorconfig" Link=".editorconfig" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="BenchmarkDotNet" Version="0.13.11" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Jakar.AppLogger.Common\Jakar.AppLogger.Common.csproj" />
	</ItemGroup>

</Project>
