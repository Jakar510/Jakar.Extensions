<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
		<NoWarn>$(NoWarn);1591</NoWarn>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<RepositoryUrl>https://github.com/Jakar510/Jakar.Extensions</RepositoryUrl>
		<RepositoryType>GitHub</RepositoryType>
		<PackageIcon>icon.png</PackageIcon>
		<Description>Extensions to help with development.</Description>
		<Copyright>MIT</Copyright>
		<PackageTags>Xml Extensions Jakar</PackageTags>
		<PackageReleaseNotes>

		</PackageReleaseNotes>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<PackageProjectUrl>https://github.com/Jakar510/Jakar.Extensions</PackageProjectUrl>
		<Authors>Tyler Stegmaier</Authors>
		<Company />
		<Version>1.0.0</Version>
		<EnableNETAnalyzers>true</EnableNETAnalyzers>
		<NeutralLanguage>en</NeutralLanguage>
		<PackageIconUrl />
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
	  <DefineConstants>DEBUG;TRACE</DefineConstants>
	  <DocumentationFile>bin\Release\Jakar.Xml.xml</DocumentationFile>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Jakar.Extensions" Version="6.0.0.3-beta" />
		<PackageReference Include="Sigil" Version="5.0.0" />
	</ItemGroup>

</Project>